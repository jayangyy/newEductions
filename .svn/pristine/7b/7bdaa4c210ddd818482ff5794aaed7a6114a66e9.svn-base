package cr.cdrb.web.edu.model;

import org.apache.commons.lang3.StringUtils;

/**
 *
 * @author milord
 */
public class Teacher {
    private String pid;//身份证号
    private String edu_date;//从事教育日期
    private String type;//教师类型：专职教师/兼职教师/管理人员
    private String subject;//课程类型
    private String jxjy_date;//继续教育日期
    private String hire_date;//聘用日期
    private String memo;//备注
    private String cert;//资格证书
    private String prof;//专业
    private String name;
    private String dwid;
    private String dwname;
    private String bmid;
    private String bmname;
    private String optuserid;
    private String optusername;
    private String optdate;

    /**
     * @return the pid
     */
    public String getPid() {
        return pid;
    }

    /**
     * @param pid the pid to set
     */
    public void setPid(String pid) {
        this.pid = pid;
    }

    /**
     * @return the edu_date
     */
    public String getEdu_date() {
        return StringUtils.isBlank(edu_date) ? "" : edu_date.split(" ")[0];
    }

    /**
     * @param edu_date the edu_date to set
     */
    public void setEdu_date(String edu_date) {
        this.edu_date = edu_date;
    }

    /**
     * @return the type
     */
    public String getType() {
        return type;
    }

    /**
     * @param type the type to set
     */
    public void setType(String type) {
        this.type = type;
    }

    /**
     * @return the subject
     */
    public String getSubject() {
        return subject;
    }

    /**
     * @param subject the subject to set
     */
    public void setSubject(String subject) {
        this.subject = subject;
    }

    /**
     * @return the jxjy_date
     */
    public String getJxjy_date() {
        return StringUtils.isBlank(jxjy_date) ? "" : jxjy_date.split(" ")[0];
    }

    /**
     * @param jxjy_date the jxjy_date to set
     */
    public void setJxjy_date(String jxjy_date) {
        this.jxjy_date = jxjy_date;
    }

    /**
     * @return the hire_date
     */
    public String getHire_date() {
        return StringUtils.isBlank(hire_date) ? "" : hire_date.split(" ")[0];
    }

    /**
     * @param hire_date the hire_date to set
     */
    public void setHire_date(String hire_date) {
        this.hire_date = hire_date;
    }

    /**
     * @return the memo
     */
    public String getMemo() {
        return memo;
    }

    /**
     * @param memo the memo to set
     */
    public void setMemo(String memo) {
        this.memo = memo;
    }

    /**
     * @return the cert
     */
    public String getCert() {
        return cert;
    }

    /**
     * @param cert the cert to set
     */
    public void setCert(String cert) {
        this.cert = cert;
    }

    /**
     * @return the prof
     */
    public String getProf() {
        return prof;
    }

    /**
     * @param prof the prof to set
     */
    public void setProf(String prof) {
        this.prof = prof;
    }

    /**
     * @return the dwid
     */
    public String getDwid() {
        return dwid;
    }

    /**
     * @param dwid the dwid to set
     */
    public void setDwid(String dwid) {
        this.dwid = dwid;
    }

    /**
     * @return the dwname
     */
    public String getDwname() {
        return dwname;
    }

    /**
     * @param dwname the dwname to set
     */
    public void setDwname(String dwname) {
        this.dwname = dwname;
    }

    /**
     * @return the bmid
     */
    public String getBmid() {
        return bmid;
    }

    /**
     * @param bmid the bmid to set
     */
    public void setBmid(String bmid) {
        this.bmid = bmid;
    }

    /**
     * @return the bmname
     */
    public String getBmname() {
        return bmname;
    }

    /**
     * @param bmname the bmname to set
     */
    public void setBmname(String bmname) {
        this.bmname = bmname;
    }

    /**
     * @return the optuserid
     */
    public String getOptuserid() {
        return optuserid;
    }

    /**
     * @param optuserid the optuserid to set
     */
    public void setOptuserid(String optuserid) {
        this.optuserid = optuserid;
    }

    /**
     * @return the optusername
     */
    public String getOptusername() {
        return optusername;
    }

    /**
     * @param optusername the optusername to set
     */
    public void setOptusername(String optusername) {
        this.optusername = optusername;
    }

    /**
     * @return the optdate
     */
    public String getOptdate() {
        return StringUtils.isBlank(optdate) ? "" : optdate.split(" ")[0];
    }

    /**
     * @param optdate the optdate to set
     */
    public void setOptdate(String optdate) {
        this.optdate = optdate;
    }

    /**
     * @return the name
     */
    public String getName() {
        return name;
    }

    /**
     * @param name the name to set
     */
    public void setName(String name) {
        this.name = name;
    }
    
}
