package cr.cdrb.web.edu.model;

/**
 *
 * @author milord
 */
public class Edu_Study_Content {
    private int id;
    private int newpostid;
    private int orderno;
    private String study_type;
    private String study_content;
    private String teacher;
    private int study_hours;
    private String memo;

    /**
     * @return the id
     */
    public int getId() {
        return id;
    }

    /**
     * @param id the id to set
     */
    public void setId(int id) {
        this.id = id;
    }

    /**
     * @return the newpostid
     */
    public int getNewpostid() {
        return newpostid;
    }

    /**
     * @param newpostid the newpostid to set
     */
    public void setNewpostid(int newpostid) {
        this.newpostid = newpostid;
    }

    /**
     * @return the orderno
     */
    public int getOrderno() {
        return orderno;
    }

    /**
     * @param orderno the orderno to set
     */
    public void setOrderno(int orderno) {
        this.orderno = orderno;
    }

    /**
     * @return the study_type
     */
    public String getStudy_type() {
        return study_type;
    }

    /**
     * @param study_type the study_type to set
     */
    public void setStudy_type(String study_type) {
        this.study_type = study_type;
    }

    /**
     * @return the study_content
     */
    public String getStudy_content() {
        return study_content;
    }

    /**
     * @param study_content the study_content to set
     */
    public void setStudy_content(String study_content) {
        this.study_content = study_content;
    }

    /**
     * @return the teacher
     */
    public String getTeacher() {
        return teacher;
    }

    /**
     * @param teacher the teacher to set
     */
    public void setTeacher(String teacher) {
        this.teacher = teacher;
    }

    /**
     * @return the study_hours
     */
    public int getStudy_hours() {
        return study_hours;
    }

    /**
     * @param study_hours the study_hours to set
     */
    public void setStudy_hours(int study_hours) {
        this.study_hours = study_hours;
    }

    /**
     * @return the memo
     */
    public String getMemo() {
        return memo;
    }

    /**
     * @param memo the memo to set
     */
    public void setMemo(String memo) {
        this.memo = memo;
    }
}
