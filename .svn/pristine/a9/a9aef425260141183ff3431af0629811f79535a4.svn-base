(SELECT top 1 '--【行车工种】--' AS 名称, 0 AS id FROM Z_行车工种) 
UNION (SELECT 名称, id FROM Z_行车工种 where 名称 not like '[高,动]%' or 名称 in ('动车组司机','动车组机械师')) 
UNION (SELECT top 1 '--【高铁岗位】--' AS 名称, 10000 AS id FROM Z_行车工种) 
UNION (SELECT 名称, id+10000 as id FROM Z_行车工种 where 名称 like '[高,动]%' and 名称 not in ('动车组司机','动车组机械师')) 
UNION (SELECT top 1 '--【营业线施工与特种作业】--' AS 名称, 20000 AS id FROM Z_特种作业种类) 
UNION (SELECT 名称, id+20000 as id FROM Z_特种作业种类) 
UNION (SELECT top 1 '--【其他工种】--' AS 名称, 30000 AS id FROM Z_职务名称) 
UNION (SELECT 名称, id+30000 as id FROM Z_职务名称 where id>99 and 名称 not in ('','学员','工人','工班长')) ORDER BY id