package cr.cdrb.web.edu.domains.eduplans;

import java.math.BigDecimal;

/**
 *
 * @author Jayang 
 * @see  其他费用明细
 */
public class EduOtherFees {

    public String getOther_id() {
        return other_id;
    }

    public void setOther_id(String other_id) {
        this.other_id = other_id;
    }

    public String getCost_id() {
        return cost_id;
    }

    public void setCost_id(String cost_id) {
        this.cost_id = cost_id;
    }

    public String getOther_cmt() {
        return other_cmt;
    }

    public void setOther_cmt(String other_cmt) {
        this.other_cmt = other_cmt;
    }

    public BigDecimal getOther_cost() {
        return other_cost;
    }

    public void setOther_cost(BigDecimal other_cost) {
        this.other_cost = other_cost;
    }

    ///主键
    private String other_id;
    //外键
    private String cost_id;
    //费用描述
    private String other_cmt;
    //费用
    private BigDecimal other_cost;
}
